.create table OPCTelemetry (NodeId: string, ApplicationUri: string, Status: string, SourceTimestamp: datetime, Random: real, Sawtooth: real, Sinusoid: real, Square: real, Triangle: real, Pressure: real, Temperature: real)

.create table OPCTelemetry ingestion json mapping "OPCTelemetryMapping" '[{ "column": "NodeId", "Properties":{"Path":"$.NodeId"}}, { "column": "ApplicationUri", "Properties":{"Path":"$.ApplicationUri"}}, { "column": "Status", "Properties":{"Path":"$.Status"}}, { "column": "SourceTimestamp", "Properties":{"Path":"$.SourceTimestamp"}}, { "column": "Random", "Properties":{"Path":"$.Random"}}, { "column": "Sawtooth", "Properties":{"Path":"$.Sawtooth"}}, { "column": "Sinusoid", "Properties":{"Path":"$.Sinusoid"}}, { "column": "Square", "Properties":{"Path":"$.Square"}}, { "column": "Triangle", "Properties":{"Path":"$.Triangle"}}, { "column": "Pressure", "Properties":{"Path":"$.Pressure"}}, { "column": "Temperature", "Properties":{"Path":"$.Temperature"}}]'
