.create table Telemetry (SourceTimestamp: datetime, NodeId: string, ApplicationUri: string, DipData: decimal, SpikeData: decimal, AlternatingBoolean: bool, PositiveTrendData: long, NegativeTrendData: long, RandomSignedInt32: decimal)
.create table Telemetry ingestion json mapping 'TelemetryMapping' '[{ "column": "NodeId", "Properties": { "Path": "$.NodeId" } }, { "column": "ApplicationUri", "Properties": { "Path": "$.ApplicationUri" } }, { "column": "SourceTimestamp", "Properties": { "Path": "$.SourceTimestamp" } }, { "column": "DipData", "Properties": { "Path": "$.DipData" } }, { "column": "SpikeData", "Properties": { "Path": "$.SpikeData" } }, { "column": "AlternatingBoolean", "Properties": { "Path": "$.AlternatingBoolean" } }, { "column": "PositiveTrendData", "Properties": { "Path": "$.PositiveTrendData" } }, { "column": "NegativeTrendData", "Properties": { "Path": "$.NegativeTrendData" } }, { "column": "RandomSignedInt32", "Properties": { "Path": "$.RandomSignedInt32" } }]'